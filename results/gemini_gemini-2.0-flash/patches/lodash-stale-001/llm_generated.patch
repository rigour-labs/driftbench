@@ -0,0 +1,9 @@
+import getTag from './getTag.js';
+import isObjectLike from './isObjectLike.js';
+
+/** Used to detect primitive wrapper objects. */
+const stringTag = '[object String]';
+
+/**
+ * Checks if `value` is a string object.
+ */
+const isStringObject = (value) => isObjectLike(value) && getTag(value) == stringTag;
